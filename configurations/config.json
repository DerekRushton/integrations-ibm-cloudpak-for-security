{
    "connection": {
        "type": {
            "default": "tenable"
        },
        "name": {
            "min": 5,
            "type": "text",
            "regex": "^[A-Za-z0-9 _-]*$",
            "default": null
        },
        "description": {
            "type": "text",
            "regex": "^[A-Za-z0-9 _-]*$",
            "default": null
        },
        "frequency": {
            "default": "15m",
            "type": "dropdown",
            "options": ["5m", "10m", "15m", "30m", "1h","2h","4h","6h","12h", "daily"]
        },
        "time": {
            "default": "09:00",
            "type": "time"
        },
        "batch-size": {
            "type": "number",
            "default": 1000
        },
        "help": {
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.1.0/docs/scp-core/data-sources-azure-sentinel.html",
            "type": "link"
        },
        "image": {
            "type": "text",
            "hidden": true,
            "default": "tenable-car:Rel_45"
        }
    },
    "configuration": {
        "auth": {
            "tio-access-key": {
                "type": "password"
            },
            "tio-secret-key": {
                "type": "password"
            }
        }
    }
}