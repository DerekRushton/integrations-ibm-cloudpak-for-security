{
    "connection": {
        "type": {
            "type": "connectorType",
            "displayName": "Tenable"
        },
        "frequency": {
            "default": "15m",
            "type": "dropdown",
            "options": ["5m", "10m", "15m", "30m", "1h","2h","4h","6h","12h", "daily"]
        },
        "time": {
            "default": "09:00",
            "type": "time"
        },
        "help": {
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.6.0/platform/docs/scp-core/data-sources-car-tenable.html",
            "type": "link"
        },
        "image": {
            "type": "text",
            "hidden": true,
            "default": "{image_link}"
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "tio_access_key": {
                "type": "password"
            },
            "tio_secret_key": {
                "type": "password"
            }
        },
        "parameter": {
            "type": "fields",
            "batch_size": {
                "type": "number",
                "default": 100
            },
            "verbose": {
                "type": "number",
                "default": 1
            },
            "since": {
                "type": "number",
                "default": 0
            }
        }
    }
}